<?xml version="1.0" encoding="UTF-8"?>


<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1"> 
         
         <servlet>
         	<servlet-name>login</servlet-name>
         	<servlet-class>admin.employee.servlet.LoginServlet</servlet-class>
          </servlet>
          
          <servlet-mapping>
          	<servlet-name>login</servlet-name>
          	<url-pattern>/log</url-pattern>
          </servlet-mapping>
         
         <servlet>
         	<servlet-name>EmployeeAdd</servlet-name>
         	<servlet-class>admin.employee.EmployeeAdd</servlet-class>
          </servlet>
         
         <servlet-mapping>
          	<servlet-name>EmployeeAdd</servlet-name>
          	<url-pattern>/insert</url-pattern>
          </servlet-mapping>
          
          <servlet>
         	<servlet-name>UpdateEmployee</servlet-name>
         	<servlet-class>admin.employee.servlet.UpdateEmployeeServlet</servlet-class>
          </servlet>
         
         <servlet-mapping>
          	<servlet-name>UpdateEmployee</servlet-name>
          	<url-pattern>/update</url-pattern>
          </servlet-mapping>
          
          <servlet>
         	<servlet-name>DeleteEmployee</servlet-name>
         	<servlet-class>admin.employee.servlet.DeleteEmployeeServlet</servlet-class>
          </servlet>
         
         <servlet-mapping>
          	<servlet-name>DeleteEmployee</servlet-name>
          	<url-pattern>/delete</url-pattern>
          </servlet-mapping>
          
          <servlet>
         	<servlet-name>AddKPI</servlet-name>
         	<servlet-class>admin.employee.servlet.AddKPIServlet</servlet-class>
          </servlet>
         
         <servlet-mapping>
          	<servlet-name>AddKPI</servlet-name>
          	<url-pattern>/Add</url-pattern>
          </servlet-mapping>
          
          <servlet>
         	<servlet-name>LoginKPI</servlet-name>
         	<servlet-class>admin.employee.servlet.LoginKPIServlet</servlet-class>
          </servlet>
         
         <servlet-mapping>
          	<servlet-name>LoginKPI</servlet-name>
          	<url-pattern>/KPI</url-pattern>
          </servlet-mapping>
          
         </web-app>
        